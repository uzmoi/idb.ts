{
  // tsconfig for tsup
  "include": ["src"],
  "exclude": ["**/*.test.ts"],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  }
}
